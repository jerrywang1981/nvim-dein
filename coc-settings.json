{
  "suggest.triggerAfterInsertEnter": true,
  "suggest.noselect": false,
  "suggest.timeout": 500,
  "suggest.minTriggerInputLength": 2,
  "suggest.echodocSupport": true,
	"suggest.snippetIndicator": "‚≠êÔ∏é",
	"suggest.enablePreview": true,
  "diagnostic.errorSign": "üò°",
	"diagnostic.warningSign": "ü§î",
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "tslint.autoFixOnSave": false,
  "tslint.jsEnable": false,
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.autoImports": true,
  "typescript.format.insertSpaceAfterConstructor": false,
  "tslint.suppressWhileTypeErrorsPresent": false,
  "tslint.alwaysShowRuleFailuresAsWarnings": false,
  "imselect.defaultInput": "com.apple.keylayout.US",
  "imselect.enableStatusItem": false,
  "eslint.packageManager": "yarn",
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "javascriptreact"
  ],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "tsserver.log": "verbose",
  "tsserver.npm": "/usr/local/bin/",
  "tsserver.trace.server": "verbose",
  "python.linting.pylintEnabled": true,
  "python.linting.flake8Enabled": false,
  "python.linting.enabled": true,
  "python.jediEnabled": false,
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "go.enable": true,
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": ["~"]
		},
  },
}
